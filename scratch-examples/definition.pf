class Set
class Any

############################ The following are equivalent
definition (in) {
  allow {
    Any in Set
  }
}

definition (in) {
  let S be : Set
  let x be : Any
  allow {
    x in S
  }
}

####### Order
class Order
let < : Order
definition {
   let S : Set
   let x in S
   let y in S
   let x != y
   either {
     x < y
     y < x
   }
}
